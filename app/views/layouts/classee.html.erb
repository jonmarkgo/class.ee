<!DOCTYPE html>
<html>
<head>
  <title>Class.ee</title>
  <%= stylesheet_link_tag "960" %>
  <%= stylesheet_link_tag "style" %>
  <%= stylesheet_link_tag "smoothness/jquery-ui-1.8.7.custom" %>
  <%= stylesheet_link_tag "jquery.ui.autocomplete.custom" %>
  <!--[if lte IE 7]>
    <%= stylesheet_link_tag "ie" %>
  <![endif]-->
  
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "tag-it" %>
  <%= javascript_include_tag "home" %>

  <!--[if lt IE 9]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
  <![endif]-->
  <%= csrf_meta_tag %>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
</head>
<body>
  <div id="wrapper" class="container_12">
    <header id="header" class="grid_12">
      <h1><%= link_to "class.ee", root_path %></h1>
    </header> <!-- end header -->
  
    <div id="content">

      <!--[if lte IE 7]>
        <div class="ie grid_7"></div>
      <![endif]-->
      <div id="main">
        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>
        <%= yield %>
      </div> <!-- end main -->

    </div> <!-- end content -->
    
    <footer id="footer" class="grid_12">
        <!-- You're free to remove the credit link in the footer, but please, please leave it there. -->
        <p>Copyright &copy; 2010 <a href="#">class.ee</a></p>
    </footer> <!-- end footer -->
    
    <div class="clear"></div>

</div> <!-- end wrapper -->
</body>
</html>
